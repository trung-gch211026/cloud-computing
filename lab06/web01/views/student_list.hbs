<div class="container mt-3 mb-3">
    <h1>Student List</h1>
<table border="1">
  <tr>
    <th colspan="4">Book List</th>
  </tr>
  <tr>
    <th>Name</th>
    <th>Age</th>
    <th>Email</th>
    <th>Address</th>
  </tr>
  {{#each students}}
    <tr>
      <th>{{name}}</th>
      <th>{{age}}</th>
      <th>{{email}}</th>
      <th>{{address}}</th>
    </tr>
  {{/each}}

</table>
</div>